#include<stdio.h>
int main()
{
    int i,j,n;
    printf("enter the value of n");
    scanf("%d",&n);
    if(n<1)
        printf("Invalid input\n");
    else{

        for(i=1;i<=n;i++)
        {
            for(j=1;j<=n;j++)
                printf("%dX%d=%d\t",j,i,i*j);
            printf("\n");
        }
    }
    return 0;
}
